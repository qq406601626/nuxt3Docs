","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":": { cors: "}]},{"type":"element","tag":"span","props":{"class":"ct-8a9a77"},"children":[{"type":"text","value":"true"}]},{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":" },"}]}]},{"type":"element","tag":"div","props":{"class":"line"},"children":[{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":"    "}]},{"type":"element","tag":"span","props":{"class":"ct-d5ccb5"},"children":[{"type":"text","value":"// Add redirect headers"}]}]},{"type":"element","tag":"div","props":{"class":"line"},"children":[{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":"    "}]},{"type":"element","tag":"span","props":{"class":"ct-cd7535"},"children":[{"type":"text","value":"'/old-page'"}]},{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":": { redirect: "}]},{"type":"element","tag":"span","props":{"class":"ct-cd7535"},"children":[{"type":"text","value":"'/new-page'"}]},{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":" },"}]}]},{"type":"element","tag":"div","props":{"class":"line"},"children":[{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":"    "}]},{"type":"element","tag":"span","props":{"class":"ct-cd7535"},"children":[{"type":"text","value":"'/old-page2'"}]},{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":": { redirect: { to: "}]},{"type":"element","tag":"span","props":{"class":"ct-cd7535"},"children":[{"type":"text","value":"'/new-page'"}]},{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":", statusCode: "}]},{"type":"element","tag":"span","props":{"class":"ct-8a9a77"},"children":[{"type":"text","value":"302"}]},{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":" } }"}]}]},{"type":"element","tag":"div","props":{"class":"line"},"children":[{"type":"element","tag":"span","props":{"class":"ct-fc30de"},"children":[{"type":"text","value":"  }"}]}]},{"type":"element","tag":"div","props":{"class":"line"},"children":[{"type":"element","tag":"span","props":{"class":"ct-6a2e0f"},"children":[{"type":"text","value":"})"}]}]}]}]}]},{"type":"element","tag":"style","children":[{"type":"text","value":".ct-8a9a77{color:#0550AE}\n.ct-cd7535{color:#0A3069}\n.ct-d5ccb5{color:#6E7781}\n.ct-fc30de{color:#24292F}\n.ct-c39f7f{color:#8250DF}\n.ct-6a2e0f{color:#953800}\n.ct-629c2b{color:#CF222E}\n.dark .ct-629c2b{color:#FF7B72}\n.dark .ct-6a2e0f{color:#FFA657}\n.dark .ct-c39f7f{color:#D2A8FF}\n.dark .ct-fc30de{color:#C9D1D9}\n.dark .ct-d5ccb5{color:#8B949E}\n.dark .ct-cd7535{color:#A5D6FF}\n.dark .ct-8a9a77{color:#79C0FF}"}]}],"toc":{"title":"","searchDepth":2,"depth":2,"links":[{"id":"client-side-only-rendering","depth":2,"text":"Client-side Only Rendering","children":[{"id":"pros","depth":3,"text":"Pros"},{"id":"cons","depth":3,"text":"Cons"},{"id":"examples","depth":3,"text":"Examples"}]},{"id":"universal-rendering","depth":2,"text":"Universal Rendering","children":[{"id":"pros-1","depth":3,"text":"Pros"},{"id":"cons-1","depth":3,"text":"Cons"},{"id":"examples-1","depth":3,"text":"Examples"}]},{"id":"summary","depth":2,"text":"Summary"},{"id":"coming-in-nuxt-3","depth":2,"text":"Coming in Nuxt 3","children":[{"id":"hybrid-rendering","depth":3,"text":"Hybrid Rendering"},{"id":"rendering-on-cdn-edge-workers","depth":3,"text":"Rendering on CDN Edge Workers"},{"id":"route-rules","depth":3,"text":"Route Rules"}]}]}},"_type":"markdown","_id":"content:2.guide:1.concepts:3.rendering.md","_source":"content","_file":"2.guide/1.concepts/3.rendering.md","_extension":"md"}